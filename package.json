{
  "name": "redux-scaffolding",
  "version": "1.2.0",
  "description": "Reusable Redux actions, reducers and selectors",
  "license": "MIT",
  "repository": "sizeight/redux-scaffolding",
  "main": "lib/redux-scaffolding.min.js",
  "keywords": [
    "react",
    "redux",
    "django",
    "reselect"
  ],
  "author": "Herman Pretorius (https://github.com/sizeight)",
  "scripts": {
    "test": "mocha --require @babel/register --recursive $(find ./src/ -name '*.spec.js') --require ./src/setup.js",
    "test:watch": "NODE_PATH=./src/ API_URL='http://127.0.0.1:8000' npm run test -- --watch",
    "webpack-dev": "webpack --watch --progress --color",
    "webpack-prod-analyse": "NODE_ENV=production ANALYSE=true webpack --watch --progress --color",
    "webpack-prod": "NODE_ENV=production webpack --progress --color",
    "build": "yarn webpack-prod",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "deep-freeze": "^0.0.1",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "expect": "^23.6.0",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^13.1.0",
    "mocha": "^5.2.0",
    "nock": "^10.0.6",
    "redux": "^4.0.1",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "rewire": "^4.0.1",
    "webpack": "^4.28.4",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.1"
  }
}
